#include <iostream>
using namespace std;

template <class T>
T sumArray(T arr[], int size) {
    T sum = 0;
    for (int i = 0; i < size; i++) {
        sum = sum + arr[i];
    }
    return sum;
}

int main() {
    int intArr[5] = {2,4,6,8,10};
    
    cout << "Sum of int array = " << sumArray(intArr, 5) << endl;
   
return 0;
}
