#include <iostream>
#include <cstring>
#include <string>
using namespace std;

template <class T>
T square(T num) {
    return num * num;
}

template <>
string square<string>(string str) {
    string result;
    result.resize(str.size() * 2);
    strcpy(&result[0], str.c_str());
    strcat(&result[0], str.c_str());
    return result;
}

int main() {
    int num = 7;
    string str = "ABC";

    cout << "Square of integer: " << square(num) << endl;
    cout << "Square of string: " << square(str) << endl;

return 0;
}
