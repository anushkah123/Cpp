#include <iostream>
using namespace std;
class SYID{
    private:
    int roll_no;
    char name[20];
    
    public:
    void accept();
    void display();
};
void SYID :: accept(){
    cout<<"Enter roll number and name:";
    cin>>roll_no>>name;
}
void SYID ::display(){
    cout<<"Name:"<<name<<endl;
    cout<<"Roll number:"<<roll_no<<endl;
}

int main(){
    SYID S[3];
    for(int i=0;i<3;i++){
        S[i].accept();
    }
    for(int i=0;i<3;i++){
        S[i].display();
    }
    return 0;
    
   
}
