#include <iostream>
using namespace std;

class SYIT{
    private:
    int Roll_no;
    char name[20];
    public:
    void accept(){
        cout<<"Enter student name and roll number:";
        cin>>name>>Roll_no;
    }
    void display(){
    cout<<"Student Name:"<<name<<endl;
    cout<<"Roll Number:"<<Roll_no;
        
    }
};

int main(){
    SYIT s1;
    s1.accept();
    s1.display();

    return 0;
}


