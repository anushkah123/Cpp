#include <iostream>
using namespace std;
class city{
    private:
    int population;
    char name[20];
    
    public:
    void accept();
    void display();
};
void city :: accept(){
    cout<<"Enter City name and population:";
    cin>>name>>population;
}
void city ::display(){
    cout<<"City:"<<name<<endl;
    cout<<"Population:"<<population<<endl;
}

int main(){
    city C[5];
    for(int i=0;i<5;i++){
        C[i].accept();
    }
    for(int i=0;i<5;i++){
        C[i].display();
    }
    return 0;
}
    
