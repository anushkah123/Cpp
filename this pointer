#include <iostream>
using namespace std;
class student{
    private:
    int roll_no;
    char name[20];
    public:
    void accept(){
        cout<<"Enter name:";
        cin>>this ->name;
        cout<<"Enter roll number:";
        cin>>this ->roll_no;
        
    }
    void display(){
        this -> accept();
        cout<<"Name:"<<name;
        cout<<"\nRoll number:"<<roll_no;
    }
};
int main(){
    student S1;
    S1.display();
    
    return 0;
    
}
