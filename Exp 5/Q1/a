#include <iostream>
using namespace std;

class Sum {
    int n = 10;
    int result = 0;

public:
    Sum() {
        for (int i = 1; i <= n; i++) {
            result += i;
        }
    }

    void display() {
        cout << "Sum of numbers from 1 to " << n << " is " << result << endl;
    }
};

int main() {
    Sum s;
    s.display();
    return 0;
}
