#include <iostream>
using namespace std;

class book{
    public:
    string b_name;
    int b_price;
    int b_pages;
    
    public:
    void accept(){
        cout<<"Enter book name, price and pages:";
        cin>>b_name>>b_price>>b_pages;
    }
    
    void display(){
        cout<<"Book Name:"<<b_name<<endl;
        cout<<"Book Price:"<<b_price<<endl;
        cout<<"Book Pages:"<<b_pages;
    }
    
};
int main(){
    book b1,b2;
    b1.accept();
    b2.accept();
    
    
    if (b1.b_price >b2.b_price){
        cout<<"Greater price:"<<endl;
        b1.display();
        
    }
    else
    cout<<"Greater price:";
        b2.display();
        
    return 0;
}
